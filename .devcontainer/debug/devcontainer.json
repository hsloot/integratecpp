{
    "name": "debug",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "runArgs": [
        "--security-opt", "seccomp=unconfined"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"DavidAnson.vscode-markdownlint",
				"REditorSupport.r",
				"streetsidesoftware.code-spell-checker",
				"ms-vscode.cpptools",
				"EditorConfig.EditorConfig",
				"redhat.vscode-yaml"
			]
		}
	}
}
